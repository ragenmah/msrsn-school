<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/white_background"
    xmlns:android="http://schemas.android.com/apk/res/android">
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ragenmah99.msrsnproject.Home"
    android:orientation="vertical"
    android:paddingTop="65dp"

    android:gravity="center"
    android:id="@+id/ll">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="219dp"
            android:clipToPadding="false"
            android:gravity="center"
            android:orientation="horizontal"
            android:background="@drawable/white_background"
          >
            <!--#002171-->
            <android.support.v7.widget.CardView
                android:id="@+id/noteID"
                android:layout_width="160dp"
                android:layout_height="200dp"
                android:layout_margin="10dp"
                android:clickable="true"

                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:background="#8c50e3"
                    android:gravity="center"
                    android:orientation="vertical">

                    <!--<ImageView-->
                        <!--android:layout_width="170dp"-->
                        <!--android:layout_height="164dp"-->
                        <!--android:background="@drawable/notebooks"-->
                        <!--android:padding="1dp" />-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:text="NOTES"
                        android:textSize="17dp"
                        android:textStyle="bold"
                        android:textColor="@color/white"
                        />
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/questionID"
                android:layout_width="160dp"
                android:layout_height="200dp"
                android:layout_margin="10dp"
                android:clickable="true"

                >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="#51e2b7"
                    android:gravity="center"
                    android:orientation="vertical">

                    <!--<ImageView-->
                        <!--android:layout_width="170dp"-->
                        <!--android:layout_height="164dp"-->
                        <!--android:background="@drawable/oldquestion"-->
                        <!--android:padding="1dp" />-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:text="OLD QUESTIONS"
                        android:textSize="17dp"
                        android:textStyle="bold"
                        android:textColor="@color/white"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="215dp"
            android:clipToPadding="false"
            android:gravity="center"
            android:orientation="horizontal"
            android:background="@drawable/white_background"
            >
            <!--#51e2b7-->
            <android.support.v7.widget.CardView
                android:id="@+id/syllabusID"
                android:layout_width="160dp"
                android:layout_height="200dp"

                android:layout_margin="10dp"
                android:clickable="true"
                >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="200dp"
                    android:background="#bf360c"
                    android:gravity="center"
                    android:orientation="vertical">

                    <!--<ImageView-->
                        <!--android:layout_width="170dp"-->
                        <!--android:layout_height="164dp"-->
                        <!--android:background="@drawable/syllabus"-->
                        <!--android:padding="1dp" />-->
                    <!--  <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:layout_margin="2dp"
                        android:background="@color/lightgray" />
                    -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:text="SYLLABUS"
                        android:textSize="17dp"
                        android:textStyle="bold"
                        android:textColor="@color/white"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/booksID"
                android:layout_width="160dp"
                android:layout_height="200dp"
                android:layout_margin="10dp"

                android:clickable="true"
>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="200dp"
                    android:background="#004c40"
                    android:gravity="center"
                    android:orientation="vertical">

                    <!--<ImageView-->
                        <!--android:layout_width="170dp"-->
                        <!--android:layout_height="164dp"-->
                        <!--android:background="@drawable/bookss"-->
                        <!--android:padding="1dp" />-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:text="BOOKS"
                        android:textSize="17dp"
                        android:textStyle="bold"
                        android:fontFamily="Garamond"
                        android:textColor="@color/white"
                        />
                </LinearLayout>
            </android.support.v7.widget.CardView>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="215dp"
            android:clipToPadding="false"
            android:gravity="center"
            android:orientation="horizontal"
            android:background="@drawable/white_background"
             >
            <!--#4286f4-->
            <android.support.v7.widget.CardView
                android:id="@+id/contactID"
                android:layout_width="160dp"
                android:layout_height="200dp"

                android:layout_margin="10dp"
                android:clickable="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:background="#1da33e"
                    android:gravity="center"
                    android:orientation="vertical">
                    <!--#004c40-->
                    <!--<ImageView-->
                        <!--android:layout_width="150dp"-->
                        <!--android:layout_height="154dp"-->
                        <!--android:background="@drawable/contacts"-->
                        <!--android:padding="1dp" />-->



                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:text="CONTACT"
                        android:textSize="17dp"
                        android:textStyle="bold"
                        android:fontFamily="Frutiger"
                        android:textColor="@color/white"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/miscID"
                android:layout_width="160dp"
                android:layout_height="200dp"
                android:layout_margin="10dp"

                android:clickable="true"
             >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="200dp"
                    android:background="#4352f9"
                    android:gravity="center"
                    android:orientation="vertical">
                    <!--#4a41f4-->
                    <!--<ImageView-->
                        <!--android:layout_width="160dp"-->
                        <!--android:layout_height="164dp"-->
                        <!--android:background="@drawable/misc"-->
                        <!--android:padding="1dp" />-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:text="MISCELLENOUS"
                        android:textSize="17dp"
                        android:textStyle="bold"
                        android:fontFamily="Helvetica"
                        android:textColor="@color/white"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>


        </LinearLayout>
</LinearLayout>
</ScrollView>


